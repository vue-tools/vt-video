{
  "name": "vt-video",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-tools/vt-video"
  },
  "main": "dist/index.vue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-tools/vt-video/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-vue-jsx",
      "transform-runtime"
    ]
  },
  "homepage": "https://github.com/vue-tools/vt-video#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "vue": "^2.1.6",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "copy": "rm -fr dist && cp -rf src dist",
    "compile": "npm run copy && node_modules/.bin/babel dist -d dist"
  }
}
